{
	"manifest_version": 2,
	"name": "New Tab URL",
	"description": "Override new tab settings to open a specified URL",
	"version": "1.0.0",
	"icons": {
		"64": "icons/icon.png"
	},
    "chrome_url_overrides": {
        "newtab": "new-tab/index.html"
    },
	"options_ui": {
		"page": "options/index.html"
	},
    "permissions": [
        "storage"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "@new-tab-override"
        }
    }
}